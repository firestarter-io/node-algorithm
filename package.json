{
	"name": "node-algorithm",
	"version": "1.0.0",
	"description": "Nodejs server / burn code",
	"main": "src/server.ts",
	"scripts": {
		"server": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
		"start:debug": "FORCE_COLOR=3 node --inspect -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
		"start": "nodemon",
		"start:pm2": "pm2 start npm --name firestarter --watch -- run start:debug",
		"launch": "pm2 start --name firestarter npm -- start",
		"test": "jest --watch",
		"build": "tsc",
		"docs": "opener http://localhost:8000 && mkdocs serve",
		"docs:deploy": "mkdocs gh-deploy",
		"unimported": "unimported"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "npm run start:debug",
		"ext": "ts"
	},
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Node: Nodemon",
			"processId": "${command:PickProcess}",
			"restart": true,
			"protocol": "inspector"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/firestarter-io/node-algorithm.git"
	},
	"author": "slutske22",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/firestarter-io/node-algorithm/issues"
	},
	"homepage": "https://github.com/firestarter-io/node-algorithm#readme",
	"devDependencies": {
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-typescript": "^7.15.0",
		"@types/express": "^4.17.12",
		"@types/jest": "^27.0.1",
		"@types/leaflet": "^1.7.3",
		"@types/lodash": "^4.14.170",
		"@types/mathjs": "^9.4.2",
		"@types/node": "^14.14.19",
		"chalk": "^4.1.2",
		"nodemon": "^2.0.9",
		"opener": "^1.5.2",
		"pm2": "^5.1.0",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.5",
		"unimported": "^1.19.0"
	},
	"dependencies": {
		"@types/winston": "^2.4.4",
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"canvas": "^2.8.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"jest": "^27.1.1",
		"leaflet": "^1.7.1",
		"lodash": "^4.17.21",
		"mathjs": "^9.4.3",
		"module-alias": "^2.2.2",
		"node-fetch": "^2.6.1",
		"tsconfig-paths": "^3.10.1",
		"uuid": "^8.3.2",
		"winston": "^3.3.3",
		"xyz-affair": "^0.9.1"
	}
}
