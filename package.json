{
	"name": "node-algorithm",
	"version": "1.0.0",
	"description": "Nodejs server / burn code",
	"main": "index.js",
	"scripts": {
		"server": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
		"start:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
		"start": "nodemon",
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "npm run start:debug",
		"ext": "ts"
	},
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Node: Nodemon",
			"processId": "${command:PickProcess}",
			"restart": true,
			"protocol": "inspector"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/firestarter-io/node-algorithm.git"
	},
	"author": "slutske22",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/firestarter-io/node-algorithm/issues"
	},
	"homepage": "https://github.com/firestarter-io/node-algorithm#readme",
	"devDependencies": {
		"@types/esri-leaflet": "^2.1.6",
		"@types/express": "^4.17.9",
		"@types/leaflet": "^1.7.0",
		"@types/mathjs": "^6.0.11",
		"@types/node": "^14.14.19",
		"nodemon": "^2.0.6",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"canvas": "^2.6.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"esri-leaflet": "^2.5.3",
		"express": "^4.17.1",
		"leaflet": "^1.7.1",
		"leaflet-geometryutil": "^0.9.3",
		"mathjs": "^9.2.0",
		"module-alias": "^2.2.2",
		"node-fetch": "^2.6.1",
		"tsconfig-paths": "^3.9.0",
		"xyz-affair": "^0.9.1"
	}
}
