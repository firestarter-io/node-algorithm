{
	"name": "node-algorithm",
	"version": "1.0.0",
	"description": "Nodejs server / burn code",
	"main": "index.js",
	"scripts": {
		"start": "node -r ts-node/register ./src/server.ts",
		"start:debug": "node --inspect=5858 -r ts-node/register ./src/server.ts",
		"dev": "nodemon",
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "npm start",
		"ext": "ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/firestarter-io/node-algorithm.git"
	},
	"author": "slutske22",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/firestarter-io/node-algorithm/issues"
	},
	"homepage": "https://github.com/firestarter-io/node-algorithm#readme",
	"devDependencies": {
		"@types/express": "^4.17.9",
		"@types/leaflet": "^1.5.19",
		"@types/node": "^14.14.19",
		"nodemon": "^2.0.6",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"canvas": "^2.6.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"leaflet": "^1.7.1",
		"leaflet-geometryutil": "^0.9.3",
		"xyz-affair": "^0.9.1"
	}
}
